<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Panel"
    title="Course">
    <items jcr:primaryType="cq:WidgetCollection">
        <course
            jcr:primaryType="cq:Widget"
            collapsed="{Boolean}false"
            collapsible="{Boolean}false"
            xtype="dialogfieldset">
            <items jcr:primaryType="cq:WidgetCollection">
                <instructor
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Instructor"
                    name="./instructor"
                    rootPath="/home/users"
                    xtype="pathfield"/>
                <publishDate
                    jcr:primaryType="cq:Widget"
                    ctCls="cq-propsdialog-ontime"
                    fieldLabel="Publish Date"
                    name="./publishDate"
                    xtype="datetime"/>
                <industry
                    jcr:primaryType="cq:Widget"
                    cls="cq-propsdialog-tags"
                    fieldLabel="Industry"
                    name="./industry"
                    namespaces="[wda]"
                    xtype="tags"/>
                <category
                    jcr:primaryType="cq:Widget"
                    cls="cq-propsdialog-tags"
                    fieldLabel="Category"
                    name="./category"
                    namespaces="[wda]"
                    xtype="tags"/>
                <oldPrice
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Old Price"
                    name="./oldPrice"
                    xtype="numberfield"/>
                <oldPriceHint
                    jcr:primaryType="cq:Widget"
                    defaultValue="Long"
                    name="./oldPrice@TypeHint"
                    value="Long"
                    xtype="hidden"/>
                <discount
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Discount(%)"
                    name="./discount"
                    xtype="numberfield"/>
                <discountHint
                    jcr:primaryType="cq:Widget"
                    defaultValue="Long"
                    name="./discount@TypeHint"
                    value="Long"
                    xtype="hidden"/>
                <lecture
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Lectures"
                    name="./lecture"
                    xtype="numberfield"/>
                <lectureHint
                    jcr:primaryType="cq:Widget"
                    defaultValue="decimal"
                    name="./lecture@TypeHint"
                    value="decimal"
                    xtype="hidden"/>
                <videoLength
                    jcr:primaryType="cq:Widget"
                    defaultValue=""
                    fieldLabel="Video"
                    name="./videolength"
                    xtype="textfield"/>
                <items jcr:primaryType="cq:WidgetCollection">
                    <english
                        jcr:primaryType="cq:Widget"
                        boxLabel="English"
                        fieldLabel="Language"
                        name="./en"
                        type="checkbox"
                        xtype="selection"/>
                    <malay
                        jcr:primaryType="cq:Widget"
                        boxLabel="Malay"
                        name="./ms"
                        type="checkbox"
                        xtype="selection"/>
                    <chinese
                        jcr:primaryType="cq:Widget"
                        boxLabel="Chinese"
                        name="./zh"
                        type="checkbox"
                        xtype="selection"/>
                    <tamil
                        jcr:primaryType="cq:Widget"
                        boxLabel="Tamil"
                        name="./ta"
                        type="checkbox"
                        xtype="selection"/>
                </items>
                <skillList
                    jcr:primaryType="cq:Widget"
                    defaultValue="0"
                    fieldLabel="Skill Level"
                    name="./skilllevel"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <one
                            jcr:primaryType="nt:unstructured"
                            text="Beginner"
                            value="Beginner"/>
                        <two
                            jcr:primaryType="nt:unstructured"
                            text="Intermediate"
                            value="Intermediate"/>
                        <three
                            jcr:primaryType="nt:unstructured"
                            text="Professional"
                            value="Professional"/>
                        <four
                            jcr:primaryType="nt:unstructured"
                            text="All Levels"
                            value="All Levels"/>
                    </options>
                </skillList>
                <accessList
                    jcr:primaryType="cq:Widget"
                    defaultValue="0"
                    fieldLabel="Access Rights"
                    name="./accessrights"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <one
                            jcr:primaryType="nt:unstructured"
                            text="Lifetime access"
                            value="lifetime Access"/>
                        <two
                            jcr:primaryType="nt:unstructured"
                            text="1-year"
                            value="1-year"/>
                        <three
                            jcr:primaryType="nt:unstructured"
                            text="2-years"
                            value="2-years"/>
                        <four
                            jcr:primaryType="nt:unstructured"
                            text="5-years"
                            value="5-years"/>
                    </options>
                </accessList>
                <warranty
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Warranty"
                    name="./warranty"
                    type="checkbox"
                    value="Boolean"
                    xtype="selection"/>
                <warrantyHint
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./warranty@TypeHint"
                    value="Boolean"
                    xtype="hidden"/>
                <ios
                    jcr:primaryType="cq:Widget"
                    fieldLabel="iOS"
                    name="./ios"
                    type="checkbox"
                    value="Boolean"
                    xtype="selection"/>
                <iosHint
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./ios@TypeHint"
                    value="Boolean"
                    xtype="hidden"/>
                <android
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Android"
                    name="./android"
                    type="checkbox"
                    value="Boolean"
                    xtype="selection"/>
                <androidHint
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./android@TypeHint"
                    value="Boolean"
                    xtype="hidden"/>
                <certificate
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Certificate"
                    name="./certificate"
                    type="checkbox"
                    value="boolean"
                    xtype="selection"/>
                <certHint
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./certificate@TypeHint"
                    value="Boolean"
                    xtype="hidden"/>
            </items>
        </course>
    </items>
</jcr:root>
